{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "Output": "",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#3A86FF",
          "leading_diamond": " ",
          "style": "diamond",
          "type": "os"
        },
        {
          "foreground": "#33DD2D",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#fffb38",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#0184bc",
          "style": "plain",
          "type": "dotnet"
        },
        {
          "foreground": "#6CA35E",
          "style": "plain",
          "type": "node"
        },
        {
          "foreground": "#9ec3f0",
          "properties": {
            "display_id": true,
            "display_name": true,
            "info_separator": " @ ",
            "prefix": " \ufd03 "
          },
          "style": "powerline",
          "type": "az"
        },
        {
          "foreground": "#b55aee",
          "properties": {
            "style": "austin",
            "threshold": 10
          },
          "style": "plain",
          "type": "executiontime"
        },
        {
          "foreground": "#D75F00",
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{if .Root}}(Admin) {{end}}{{.Folder}}",
  "final_space": true,
  "version": 1
}
